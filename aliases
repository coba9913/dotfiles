alias c='clear'
alias x='exit'
alias g='git'
alias ga='git add'
alias gf='git fetch'
alias gc='git commit'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gps='git push'
alias gpl='git pull'
alias gs='git status'
alias topten='print -l -- ${(o)history%% *} | uniq -c | sort -nr | head -n 10'
alias p='python'
alias stop='service supervisor stop || service nginx stop'
alias start='service supervisor start || service nginx start'
alias sra='supervisorctl restart all'
alias ssa='supervisorctl stop all'
alias smashtokill="echo 'drop database ftp_service; create database ftp_service;' | python manage.py dbshell && python manage.py migrate && python seed.py && python manage.py createsuperuser"
