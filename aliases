alias c='clear'
alias x='exit'
alias g='git'
alias ga='git add'
alias gf='git fetch'
alias gc='git commit'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gps='git push'
alias gpl='git pull'
alias gs='git status'
alias topten='print -l -- ${(o)history%% *} | uniq -c | sort -nr | head -n 10'
alias sstop='service supervisor stop'
alias nstop='service nginx stop'
alias sstart='service supervisor start'
alias nstart='service nginx start'
alias sra='supervisorctl restart all'
alias ssa='supervisorctl stop all'
